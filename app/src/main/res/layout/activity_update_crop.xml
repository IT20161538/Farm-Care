<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundlogin"
    tools:context=".activity_update_crop">

    <View
        android:id="@+id/divider"
        android:layout_width="409dp"
        android:layout_height="2dp"
        android:background="@color/black"
        android:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.635"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.106" />

    <ImageView
        android:id="@+id/im_userprofile"
        android:layout_width="46dp"
        android:layout_height="60dp"
        android:layout_marginTop="7dp"
        android:contentDescription="TODO"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_update_crop"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/userprofile2"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="66dp"
        android:layout_height="44dp"
        android:layout_marginTop="7dp"
        android:contentDescription="TODO"
        app:layout_constraintEnd_toStartOf="@+id/tv_update_crop"
        app:layout_constraintHorizontal_bias="0.01"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_home"
        tools:ignore="ImageContrastCheck,ImageContrastCheck" />

    <TextView
        android:id="@+id/tv_update_crop"
        android:layout_width="176dp"
        android:layout_height="39dp"
        android:layout_marginTop="20dp"
        android:background="#018786"
        android:text="@string/tv_update_crop"
        android:textColor="@color/white"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.541"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_cname_update"
        android:layout_width="130dp"
        android:layout_height="39dp"
        android:layout_marginStart="2dp"
        android:layout_marginBottom="28dp"
        android:text="@string/tv_cname_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_charvest_update"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_charvest_update"
        android:layout_width="130dp"
        android:layout_height="39dp"
        android:layout_marginStart="2dp"
        android:layout_marginBottom="9dp"
        android:text="@string/tv_charvest_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_croporder_update"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_croporder_update"
        android:layout_width="130dp"
        android:layout_height="39dp"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="13dp"
        android:text="@string/tv_croporder_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_cremainder_update"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_cremainder_update"
        android:layout_width="130dp"
        android:layout_height="39dp"
        android:layout_marginBottom="30dp"
        android:text="@string/tv_cremainder_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_cpickup_date_update"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_cpickup_date_update"
        android:layout_width="130dp"
        android:layout_height="39dp"
        android:layout_marginStart="2dp"
        android:layout_marginBottom="16dp"
        android:text="@string/tv_cpickup_date_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_cropup_update"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_cropup_update"
        android:layout_width="130dp"
        android:layout_height="39dp"
        android:layout_marginBottom="143dp"
        android:text="@string/tv_cropup_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/et_cname_update"
        android:layout_width="197dp"
        android:layout_height="48dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="9dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/et_cname_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_charvest_update"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/et_charvest_update"
        android:layout_width="197dp"
        android:layout_height="48dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/et_charvest_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_croporder_update"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/et_croporder_update"
        android:layout_width="197dp"
        android:layout_height="48dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="4dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/et_croporder_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_cremainder_update"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/et_cremainder_update"
        android:layout_width="197dp"
        android:layout_height="48dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="11dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/et_cremainder_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_cpickup_date_update"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/et_cpickup_date_update"
        android:layout_width="197dp"
        android:layout_height="48dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/et_cpickup_date_update"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/et_crop_unitp_update"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/btn_update_crop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="45dp"
        android:backgroundTint="@color/teal_700"
        android:text="@string/btn_update_crop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider" />

    <EditText
        android:id="@+id/et_crop_unitp_update"
        android:layout_width="184dp"
        android:layout_height="52dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="146dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/et_crop_unitp_update"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>