<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/single_viewCrop_section"
        android:layout_width="125dp"
        android:layout_height="35dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="32dp"
        android:text="Section"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_singleCrop_section"
        android:layout_width="141dp"
        android:layout_height="36dp"
        android:layout_marginEnd="36dp"
        android:text="TextView"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/single_viewCrop_section"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/single_viewCrop_type"
        android:layout_width="125dp"
        android:layout_height="35dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="28dp"
        android:text="Crop Type"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/single_viewCrop_section" />

    <TextView
        android:id="@+id/tv_singleCrop_type"
        android:layout_width="141dp"
        android:layout_height="36dp"
        android:layout_marginEnd="36dp"
        android:text="TextView"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/single_viewCrop_type"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/single_viewCrop_lpd"
        android:layout_width="125dp"
        android:layout_height="35dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="28dp"
        android:text="Last Pesticide Date"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/single_viewCrop_type" />

    <TextView
        android:id="@+id/single_viewCrop_dpd"
        android:layout_width="125dp"
        android:layout_height="35dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="28dp"
        android:text="Due Pesticide Date"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/single_viewCrop_lpd" />

    <TextView
        android:id="@+id/tv_singleCrop_lpd"
        android:layout_width="141dp"
        android:layout_height="36dp"
        android:layout_marginEnd="36dp"
        android:text="TextView"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/single_viewCrop_lpd"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/tv_singleCrop_dpd"
        android:layout_width="141dp"
        android:layout_height="36dp"
        android:layout_marginEnd="36dp"
        android:text="TextView"
        android:textColor="@color/black"
        app:layout_constraintBaseline_toBaselineOf="@+id/single_viewCrop_dpd"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>